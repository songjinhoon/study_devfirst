{{>layout/header}}

	<h1>webpack</h1>
	<button id="testBtn">버튼수정반영</button>
	<div class="container">
		<table class="table table-hover">
			<thead>
		      	<tr>
			        <th></th>
			        <th>제목</th>
			        <th>내용</th>
			        <th>작성일</th>
			        <th>수정일</th>
		        </tr>
	        </thead>
	    	<tbody>
	    		{{#map.posts}}
			    <tr>
			        <td>체크</td>
			        <td>{{title}}</td>
			        <td>{{content}}</td>
			        <td>{{createdDate}}</td>
			        <td>{{modifiedDate}}</td>
				</tr>			
				{{/map.posts}}     
		    </tbody>
		</table>
	</div>
    <div class="pageDiv">
        {{pageProcess map.pageInfo}}
    </div>

{{>layout/footer}}